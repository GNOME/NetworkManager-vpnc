#, c-format
msgid "unhandled property '%s' type %s"
msgstr "nieobsługiwana własność \"%s\" typu \"%s\""

#: ../src/nm-vpnc-service.c:176
#, c-format
msgid "property '%s' invalid or not supported"
msgstr "własność \"%s\" jest nieprawidłowa lub nieobsługiwana"

