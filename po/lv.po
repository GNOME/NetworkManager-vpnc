msgid ""
"NAT traversal method to use\n"
"config: NAT Traversal Mode <natt/none/force-natt/cisco-udp>"
msgstr ""
"Kādu NAT šķērsošanas metodi izmantot\n"
"config: NAT Traversal Mode <natt/none/force-natt/cisco-udp>"

#: ../properties/nm-vpnc-dialog.ui.h:12
msgid "_IKE DH Group:"
msgstr "IKE DH grupa:"

