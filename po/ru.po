#: ../src/nm-vpnc-service.c:204
#, c-format
msgid "property “%s” invalid or not supported"
msgstr "недействительный или неподдерживаемый параметр \"%s\""

#: ../src/nm-vpnc-service.c:219
#, c-format
msgid "property “%s” contains a newline character"
msgstr "свойство “%s” содержит символ новой строки"

#: ../src/nm-vpnc-service.c:231
#, c-format
msgid "property “%s” file path “%s” is not absolute or does not exist"
msgstr ""
"путь к файлу “%s” свойства “%s” не является абсолютным или не существует"

#: ../src/nm-vpnc-service.c:244
#, c-format
msgid "invalid integer property “%s” or out of range [%d -> %d]"
msgstr ""
"целочисленный параметр “%s” недействителен, либо вне диапазона [%d -> %d]"

#: ../src/nm-vpnc-service.c:254
#, c-format
msgid "invalid boolean property “%s” (not yes or no)"
msgstr ""
"недействительный логический параметр “%s” (значениями не являются «да» или «не"
"т»)"

#: ../src/nm-vpnc-service.c:261
#, c-format
msgid "unhandled property “%s” type %d"
msgstr "необработанный параметр “%s” с типом %d"

#: ../src/nm-vpnc-service.c:278
msgid "No VPN configuration options."
msgstr "Нет параметров конфигурации для VPN"

#: ../src/nm-vpnc-service.c:304
msgid "No VPN secrets!"
msgstr "Секреты VPN отсутствуют"

#: ../src/nm-vpnc-service.c:649
msgid "Could not find vpnc binary."
msgstr "Не удалось найти бинарный файл vpnc"

#: ../src/nm-vpnc-service.c:767
#, c-format
msgid "Config option “%s” invalid or unknown."
msgstr "Недействительный или неизвестный параметр конфигурации “%s”."

#: ../src/nm-vpnc-service.c:802
#, c-format
msgid "Config option “%s” not an integer."
msgstr "Параметр конфигурации “%s” не является целым числом"

#: ../src/nm-vpnc-service.c:990
msgid "vpnc does not support interactive requests"
msgstr "vpnc не поддерживает интерактивные запросы"

#: ../src/nm-vpnc-service.c:1014
msgid "Could not use new secrets as interactive mode is disabled."
msgstr ""
"Невозможно использовать новые секреты, т.к. интерактивный режим отключён"

#: ../src/nm-vpnc-service.c:1023 ../src/nm-vpnc-service.c:1097
msgid ""
"Could not process the request because the VPN connection settings were "
"invalid."
msgstr ""
"Не удалось обработать запрос, параметры подключения VPN оказались недействител"
"ьными"

#: ../src/nm-vpnc-service.c:1031
msgid ""
"Could not process the request because no pending authentication is required."
msgstr ""
"Не удалось обработать запрос, т.к. дополнительная аутентификация не требуется"

#: ../src/nm-vpnc-service.c:1042
#, c-format
msgid ""
"Could not process the request because the requested info “%s” was not "
"provided."
msgstr ""
"Не удалось обработать запрос, т.к. запрошенные сведения “%s” не были предостав"
"лены"

#: ../src/nm-vpnc-service.c:1240
msgid "Don’t quit when VPN connection terminates"
msgstr "Не закрывать программу после прерывания подключения VPN"

#: ../src/nm-vpnc-service.c:1241
msgid "Enable verbose debug logging (may expose passwords)"
msgstr ""
"Включить подробное журналирование отладки (пароли могут оказаться в открытом д"
"оступе)"

#: ../src/nm-vpnc-service.c:1242
msgid "D-Bus name to use for this instance"
msgstr "Имя D-Bus, используемое для этого экземпляра"

#: ../src/nm-vpnc-service.c:1265
msgid ""
"nm-vpnc-service provides integrated Cisco Legacy IPsec VPN capability to "
"NetworkManager."
msgstr ""
"nm-vpnc-service предоставляет встроенные возможности Cisco Legacy IPsec VPN пр"
"ограмме NetworkManager."

