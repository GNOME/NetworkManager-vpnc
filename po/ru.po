#: ../properties/nm-vpnc-dialog.ui.h:1
msgid "General"
msgstr "Основные"

#: ../properties/nm-vpnc-dialog.ui.h:2
msgid "_Gateway"
msgstr "_Шлюз"

#: ../properties/nm-vpnc-dialog.ui.h:3
msgid ""
"IP/hostname of IPsec gateway\n"
"config: IPSec gateway <gateway>"
msgstr ""
"IP / имя хоста IPsec\n"
"параметр: IPSec gateway <gateway>"

#: ../properties/nm-vpnc-dialog.ui.h:5
msgid "User na_me"
msgstr "_Имя пользователя"

#: ../properties/nm-vpnc-dialog.ui.h:6
msgid ""
"User name for the connection\n"
"config: Xauth username <user_name>"
msgstr ""
"Имя пользователя для подключения\n"
"параметр: Xauth имя пользователя <user_name>"

#: ../properties/nm-vpnc-dialog.ui.h:8
msgid "User _password"
msgstr "Пароль _пользователя"

#: ../properties/nm-vpnc-dialog.ui.h:9
msgid ""
"User password for the connection\n"
"config: Xauth password <password>"
msgstr ""
"Пароль пользователя для подключения\n"
"параметр: Xauth пароль <password>"

#: ../properties/nm-vpnc-dialog.ui.h:11
msgid "G_roup name"
msgstr "Имя г_руппы"

#: ../properties/nm-vpnc-dialog.ui.h:12
msgid ""
"Group name\n"
"config: IPSec ID <group_name>"
msgstr ""
"Имя группы\n"
"параметр: IPSec ID <group_name>"

#: ../properties/nm-vpnc-dialog.ui.h:14
msgid "Gro_up password"
msgstr "Пароль гру_ппы"

#: ../properties/nm-vpnc-dialog.ui.h:15
msgid ""
"Group password\n"
"config: IPSec secret <group_password>"
msgstr ""
"Групповой пароль\n"
"параметр: IPSec ключ <group_password>"

#: ../properties/nm-vpnc-dialog.ui.h:17
msgid "Sho_w passwords"
msgstr "_Показывать пароли"

#: ../properties/nm-vpnc-dialog.ui.h:18
msgid "Use _hybrid authentication"
msgstr "Использовать _гибридную аутентификацию"

#: ../properties/nm-vpnc-dialog.ui.h:19
msgid ""
"Enable hybrid authentication, i.e. use certificate in addition to password.\n"
"config: IKE Authmode hybrid"
msgstr ""
"Включить гибридную аутентификацию, т.е. использовать сертификат в дополнение "
"к паролю.\n"
"параметр: Гибридный режим IKE Authmode"

#: ../properties/nm-vpnc-dialog.ui.h:21
msgid "Advance_d…"
msgstr "_Дополнительно…"

#: ../properties/nm-vpnc-dialog.ui.h:22
msgid "Advanced Options"
msgstr "Дополнительные настройки"

#: ../properties/nm-vpnc-dialog.ui.h:23
msgid "Advanced Properties"
msgstr "Дополнительные свойства"

#: ../properties/nm-vpnc-dialog.ui.h:24
msgid "_Cancel"
msgstr "_Отменить"

#: ../properties/nm-vpnc-dialog.ui.h:25
msgid "_Apply"
msgstr "_Применить"

#: ../properties/nm-vpnc-dialog.ui.h:26
msgid "Identification"
msgstr "Идентификация"

#: ../properties/nm-vpnc-dialog.ui.h:27
msgid "_Domain"
msgstr "_Домен"

#: ../properties/nm-vpnc-dialog.ui.h:28
msgid ""
"(NT-)Domain name for authentication\n"
"config: Domain <domain>"
msgstr ""
"(NT-)Доменное имя для аутентификации\n"
"параметр: Домен <domain>"

#: ../properties/nm-vpnc-dialog.ui.h:30
msgid "_Vendor"
msgstr "_Производитель"

#: ../properties/nm-vpnc-dialog.ui.h:31
msgid ""
"Vendor of your IPsec gateway\n"
"config: Vendor <cisco/netscreen>"
msgstr ""
"Производитель вашего шлюза IPsec\n"
"параметр: Производитель <cisco/netscreen>"

#: ../properties/nm-vpnc-dialog.ui.h:33
msgid "Ve_rsion"
msgstr "_Версия"

#: ../properties/nm-vpnc-dialog.ui.h:34
msgid ""
"Application version to report. \n"
"config: Application version <ASCII string>"
msgstr ""
"Версия приложения для отчета. \n"
"параметр: Версия приложения <ASCII string>"

#: ../properties/nm-vpnc-dialog.ui.h:36
msgid "Transport and Security"
msgstr "Транспорт и защита"

#: ../properties/nm-vpnc-dialog.ui.h:37
msgid "Tunnel _interface name"
msgstr "Имя _интерфейса туннеля"

#: ../properties/nm-vpnc-dialog.ui.h:38
msgid ""
"Tunnel interface name to use\n"
"config: Interface name <name>"
msgstr ""
"Имя интерфейса туннеля для использования\n"
"параметр: имя интерфейса <name>"

#: ../properties/nm-vpnc-dialog.ui.h:40
msgid "_Encryption method"
msgstr "_Метод шифрования"

#: ../properties/nm-vpnc-dialog.ui.h:41
msgid ""
"Encryption method\n"
"config: nothing for Secure, “Enable Single DES” for Weak, “Enable no "
"encryption” for None"
msgstr ""
"Метод шифрования\n"
"параметр: ничего для Безопасный, \"Включить одиночный DES\" для Слабый, "
"\"Включить без шифрования\" для Нет"

#: ../properties/nm-vpnc-dialog.ui.h:43
msgid "_NAT traversal"
msgstr "_Обход NAT"

#: ../properties/nm-vpnc-dialog.ui.h:44
msgid ""
"NAT traversal method to use\n"
"config: NAT Traversal Mode <natt/none/force-natt/cisco-udp>"
msgstr ""
"Метод обхода NAT для использования\n"
"параметр: Метод обхода NAT <natt/none/force-natt/cisco-udp>"

#: ../properties/nm-vpnc-dialog.ui.h:46
msgid "_IKE DH Group"
msgstr "_Группа IKE DH"

#: ../properties/nm-vpnc-dialog.ui.h:47
msgid ""
"Name of the IKE DH group\n"
"config: IKE DH Group <dh1/dh2/dh5>"
msgstr ""
"Имя группы IKE DH\n"
"параметр: Группа IKE DH <dh1/dh2/dh5>"

#: ../properties/nm-vpnc-dialog.ui.h:49
msgid "Perfect _Forward Secrecy"
msgstr "Совершенная _прямая секретность"

#: ../properties/nm-vpnc-dialog.ui.h:50
msgid ""
"Diffie-Hellman group to use for PFS\n"
"config: Perfect Forward Secrecy <nopfs/dh1/dh2/dh5/server>"
msgstr ""
"Группа Diffie-Hellman для использования в PFS\n"
"параметр: Совершенная прямая секретность <nopfs/dh1/dh2/dh5/server>"

#: ../properties/nm-vpnc-dialog.ui.h:52
msgid "_Local port"
msgstr "_Локальный порт"

#: ../properties/nm-vpnc-dialog.ui.h:53
msgid ""
"Local ISAKMP port to use (0 means random port; 500 is vpnc’s default)\n"
"config: Local Port <0-65535>"
msgstr ""
"Локальный ISAKMP-порт для использования (0 означает случайный порт; 500 - по "
"умолчанию vpnc)\n"
"параметр: Локальный порт <0-65535>"

#: ../properties/nm-vpnc-dialog.ui.h:55
msgid "0"
msgstr "0"

#: ../properties/nm-vpnc-dialog.ui.h:56
msgid "Disable Dead _Peer Detection"
msgstr "_Отключить Dead Peer Detection"

#: ../properties/nm-vpnc-dialog.ui.h:57
msgid ""
"Disable sending DPD packets (sets timeout to 0)\n"
"config: DPD idle timeout (our side) 0"
msgstr ""
"Отключить отправку пакетов DPD (устанавливает тайм-аут на 0)\n"
"параметр: Тайм-аут простоя DPD (наша сторона) 0"

