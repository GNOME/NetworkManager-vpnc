msgstr ""
"Nama dari grup DH IKE\n"
"config: IKE DH Group <dh1/dh2/dh5>"

#: ../properties/nm-vpnc-dialog.ui.h:22
msgid "Perfect _Forward Secrecy:"
msgstr "Perfect _Forward Secrecy:"

